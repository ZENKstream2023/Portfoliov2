er\">\n    <div style=\"margin-top: 15em; padding-inline:40%; z-index:1\">\n     \n      <!-- Renderizar el formulario según el estado formType -->\n      <form v-if=\"formType === 'signin'\" @submit.prevent=\"handleSignIn\" class=\"signin\">\n\n        <img class=\"mb-4\" src=\"logofooter.png\" alt=\"\" width=\"150em\" height=\"130em\">\n        <h1 class=\"h3 mb-3 fw-normal\"></h1>\n\n        <div class=\"form-floating\">\n          <input type=\"email\" class=\"form-control\" id=\"floatingInput\" placeholder=\"name@example.com\" v-model=\"email\">\n          <label for=\"floatingInput\">Email</label>\n        </div>\n        <div class=\"form-floating\">\n          <input type=\"password\" class=\"form-control\" id=\"floatingPassword\" placeholder=\"Password\" v-model=\"password\">\n          <label for=\"floatingPassword\">Contraseña</label>\n        </div>\n\n        <button class=\"btn btn-primary w-100 py-2 mt-3\" style=\"background: linear-gradient(\n    to right,\n    #df18df,\n    #4c9ae7\n  );\" type=\"submit\">Iniciar Sesión</button>\n   <div class=\"text-center\">\n        <button class=\"btn btn-link\" @click=\"setFormType('signup')\">Crear Cuenta</button>\n      </div>\n      </form>\n      <form v-else-if=\"formType === 'signup'\" @submit.prevent=\"createAccount\" class=\"signup\">\n        <img class=\"mb-4\" src=\"logofooter.png\" alt=\"\" width=\"150em\" height=\"130em\">\n        <h1 class=\"h3 mb-3 fw-normal\"></h1>\n\n        <div class=\"form-floating\">\n          <input type=\"email\" class=\"form-control\" id=\"floatingInput\" placeholder=\"name@example.com\" v-model=\"email\">\n          <label for=\"floatingInput\">Email</label>\n        </div>\n        <div class=\"form-floating\">\n          <input type=\"password\" class=\"form-control\" id=\"floatingPassword\" placeholder=\"Password\" v-model=\"password\">\n          <label for=\"floatingPassword\">Contraseña</label>\n        </div>\n\n        <button class=\"btn btn-primary w-100 py-2 mt-3\" style=\"background: linear-gradient(\n    to right,\n    #df18df,\n    #4c9ae7\n  );\" type=\"submit\">Crear Cuenta</button>\n   <div class=\"text-center\">\n        <button class=\"btn btn-link\" @click=\"setFormType('signin')\">Entrar</button>\n      </div>\n      </form>\n    </div>\n  </div>\n</template>\n  \n\n<script>\nimport { ref} from 'vue';\nimport axios from 'axios';\nimport { useRouter } from 'vue-router';\nexport default {\n  name: \"LoginView\",\n  setup() {\n    const email = ref(null);\n    const password = ref(null);\n    const router = useRouter();\n    \n    const formType = ref('signup'); // Por defecto, muestra el formulario de registro\n    const handleSignIn = async () => {\n      try {\n        const response = await axios.post('https://localhost:3200/signin', {\n          email: email.value,\n          password: password.value\n        });\n        // Manejar la respuesta del backend si es necesario\n        console.log(response)\n        if(response.data.status === \"ok\")\n        {\n          router.push('/panel');\n        }\n      } catch (error) {\n        // Manejar el error si la solicitud falla\n        console.log(error)\n      }\n    };\nconst createAccount = async () => {\n      try {\n        const response = await axios.post('https://localhost:3200/signup', {\n          email: email.value,\n          password: password.value\n        });\n        // Manejar la respuesta del backend si es necesario\n        console.log(response)\n        if(response.data.status === \"ok\")\n        {\n          router.push('/panel');\n        }\n      } catch (error) {\n        // Manejar el error si la solicitud falla\n      }\n    };\n\n\nconst setFormType = (type) => {\n  formType.value = type;\n};\n\n    return {\n      email,\n      password,\n      formType,\n      handleSignIn,\n      createAccount,\n      setFormType,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.banner {\n  position: relative;\n  background-size: cover;\n  padding: 10em;\n  height: 60vh;\n  display: flex;\n  align-items: center;\n}\n</style>"],"mappings":";AAwDA,SAASA,GAAG,QAAO,KAAK;AACxB,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,YAAY;AACtC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,GAAG,CAAC,IAAI,CAAC;IACvB,MAAMM,QAAO,GAAIN,GAAG,CAAC,IAAI,CAAC;IAC1B,MAAMO,MAAK,GAAIL,SAAS,CAAC,CAAC;IAE1B,MAAMM,QAAO,GAAIR,GAAG,CAAC,QAAQ,CAAC,EAAE;IAChC,MAAMS,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC/B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,+BAA+B,EAAE;UACjEN,KAAK,EAAEA,KAAK,CAACO,KAAK;UAClBN,QAAQ,EAAEA,QAAQ,CAACM;QACrB,CAAC,CAAC;QACF;QACAC,OAAO,CAACC,GAAG,CAACJ,QAAQ;QACpB,IAAGA,QAAQ,CAACK,IAAI,CAACC,MAAK,KAAM,IAAI,EAChC;UACET,MAAM,CAACU,IAAI,CAAC,QAAQ,CAAC;QACvB;MACF,EAAE,OAAOC,KAAK,EAAE;QACd;QACAL,OAAO,CAACC,GAAG,CAACI,KAAK;MACnB;IACF,CAAC;IACL,MAAMC,aAAY,GAAI,MAAAA,CAAA,KAAY;MAC5B,IAAI;QACF,MAAMT,QAAO,GAAI,MAAMT,KAAK,CAACU,IAAI,CAAC,+BAA+B,EAAE;UACjEN,KAAK,EAAEA,KAAK,CAACO,KAAK;UAClBN,QAAQ,EAAEA,QAAQ,CAACM;QACrB,CAAC,CAAC;QACF;QACAC,OAAO,CAACC,GAAG,CAACJ,QAAQ;QACpB,IAAGA,QAAQ,CAACK,IAAI,CAACC,MAAK,KAAM,IAAI,EAChC;UACET,MAAM,CAACU,IAAI,CAAC,QAAQ,CAAC;QACvB;MACF,EAAE,OAAOC,KAAK,EAAE;QACd;MAAA;IAEJ,CAAC;IAGL,MAAME,WAAU,GAAKC,IAAI,IAAK;MAC5Bb,QAAQ,CAACI,KAAI,GAAIS,IAAI;IACvB,CAAC;IAEG,OAAO;MACLhB,KAAK;MACLC,QAAQ;MACRE,QAAQ;MACRC,YAAY;MACZU,aAAa;MACbC;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}