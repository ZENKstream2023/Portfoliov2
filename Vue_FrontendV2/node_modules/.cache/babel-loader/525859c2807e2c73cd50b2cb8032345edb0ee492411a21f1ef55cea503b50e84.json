{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport axios from 'axios';\nimport Global from '../Global';\nexport default {\n  name: \"LoginView\",\n  setup() {\n    const url = ref(Global);\n    const data = ref(null);\n    const currentYear = ref(null);\n    onMounted(() => {\n      // Obtenemos el elemento del span que mostrará el año actual\n      currentYear.value = document.getElementById(\"currentYear\");\n      // Obtenemos el año actual\n      const year = new Date().getFullYear();\n      // Actualizamos el contenido del span con el año actual\n      currentYear.value.textContent = year;\n    });\n    const handleSignIn = async () => {\n      try {\n        const response = await axios.get(url.value + \"auth/login\");\n        data.value = response.data.data;\n        const redirectTo = response.data.redirectTo;\n        if (redirectTo) {\n          window.location.href = redirectTo; // Redirige a la página de autenticación de Twitch\n        }\n      } catch (error) {\n        console.error(\"Error al iniciar sesión:\", error);\n      }\n    };\n    return {\n      handleSignIn,\n      currentYear\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","Global","name","setup","url","data","currentYear","value","document","getElementById","year","Date","getFullYear","textContent","handleSignIn","response","get","redirectTo","window","location","href","error","console"],"sources":["/home/aritz/Descargas/Vue_Frontend/src/components/Login.vue"],"sourcesContent":["<template>\n  <div class=\"banner\">\n    <div style=\"margin-top: 13em; padding-inline:40%; z-index:1\">\n      <form @submit.prevent=\"handleSignIn\">\n        <img class=\"mb-4\" src=\"logofooter.png\" alt=\"\" width=\"150em\" height=\"130em\">\n        <h1 class=\"h3 mb-3 fw-normal\"></h1>\n\n        <div class=\"form-floating\">\n          <input type=\"email\" class=\"form-control\" id=\"floatingInput\" placeholder=\"name@example.com\" v-model=\"email\">\n          <label for=\"floatingInput\">Email</label>\n        </div>\n        <div class=\"form-floating\">\n          <input type=\"password\" class=\"form-control\" id=\"floatingPassword\" placeholder=\"Password\" v-model=\"password\">\n          <label for=\"floatingPassword\">Contraseña</label>\n        </div>\n\n        <div class=\"form-check text-start my-3\">\n          <input class=\"form-check-input\" type=\"checkbox\" value=\"remember-me\" id=\"flexCheckDefault\">\n          <label class=\"form-check-label\" for=\"flexCheckDefault\">\n            Recordar\n          </label>\n        </div>\n        <button class=\"btn btn-primary w-100 py-2\" style=\"background: linear-gradient(\n    to right,\n    #df18df,\n    #4c9ae7\n  );\" type=\"submit\">Iniciar Sesión</button>\n        <p class=\"mt-4 mb-3 text-body-secondary\">&copy; <span id=\"currentYear\"></span></p>\n      </form>\n    </div>\n  </div>\n</template>\n  \n\n<script>\nimport { ref, onMounted } from 'vue';\nimport axios from 'axios';\nimport Global from '../Global'\n\nexport default {\n  name: \"LoginView\",\n  setup() {\n    const url = ref(Global);\n    const data = ref(null);\n    const currentYear = ref(null);\n\n    onMounted(() => {\n      // Obtenemos el elemento del span que mostrará el año actual\n      currentYear.value = document.getElementById(\"currentYear\");\n      // Obtenemos el año actual\n      const year = new Date().getFullYear();\n      // Actualizamos el contenido del span con el año actual\n      currentYear.value.textContent = year;\n    });\n\n    const handleSignIn = async () => {\n      try {\n        const response = await axios.get(url.value + \"auth/login\");\n        data.value = response.data.data;\n        const redirectTo = response.data.redirectTo;\n        if (redirectTo) {\n          window.location.href = redirectTo; // Redirige a la página de autenticación de Twitch\n        }\n      } catch (error) {\n        console.error(\"Error al iniciar sesión:\", error);\n      }\n    };\n\n    return {\n      handleSignIn,\n      currentYear\n    };\n  },\n};\n</script>\n\n<style scoped>\n.banner {\n  position: relative;\n  background-size: cover;\n  padding: 10em;\n  height: 60vh;\n  display: flex;\n  align-items: center;\n}\n</style>"],"mappings":"AAmCA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAU;AAE7B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,GAAE,GAAIN,GAAG,CAACG,MAAM,CAAC;IACvB,MAAMI,IAAG,GAAIP,GAAG,CAAC,IAAI,CAAC;IACtB,MAAMQ,WAAU,GAAIR,GAAG,CAAC,IAAI,CAAC;IAE7BC,SAAS,CAAC,MAAM;MACd;MACAO,WAAW,CAACC,KAAI,GAAIC,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC;MAC1D;MACA,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC;MACrC;MACAN,WAAW,CAACC,KAAK,CAACM,WAAU,GAAIH,IAAI;IACtC,CAAC,CAAC;IAEF,MAAMI,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC/B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAACZ,GAAG,CAACG,KAAI,GAAI,YAAY,CAAC;QAC1DF,IAAI,CAACE,KAAI,GAAIQ,QAAQ,CAACV,IAAI,CAACA,IAAI;QAC/B,MAAMY,UAAS,GAAIF,QAAQ,CAACV,IAAI,CAACY,UAAU;QAC3C,IAAIA,UAAU,EAAE;UACdC,MAAM,CAACC,QAAQ,CAACC,IAAG,GAAIH,UAAU,EAAE;QACrC;MACF,EAAE,OAAOI,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF,CAAC;IAED,OAAO;MACLP,YAAY;MACZR;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}