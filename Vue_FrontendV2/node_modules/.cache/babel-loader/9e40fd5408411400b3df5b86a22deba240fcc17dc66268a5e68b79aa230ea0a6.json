{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport axios from 'axios';\nimport Global from '../Global';\nexport default {\n  name: \"LoginView\",\n  setup() {\n    const url = ref(Global);\n    const data = ref(null);\n    const currentYear = ref(null);\n    onMounted(() => {\n      // Obtenemos el elemento del span que mostrará el año actual\n      currentYear.value = document.getElementById(\"currentYear\");\n      // Obtenemos el año actual\n      const year = new Date().getFullYear();\n      // Actualizamos el contenido del span con el año actual\n      currentYear.value.textContent = year;\n    });\n    const handleSignIn = async () => {\n      try {\n        const response = await axios.get(url.value + \"auth/login\");\n        data.value = response.data.data;\n        const redirectTo = response.data.redirectTo;\n        if (redirectTo) {\n          window.location.href = redirectTo; // Redirige a la página de autenticación de Twitch\n        }\n      } catch (error) {\n        console.error(\"Error al iniciar sesión:\", error);\n      }\n    };\n    return {\n      handleSignIn,\n      currentYear\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","axios","Global","name","setup","url","data","currentYear","value","document","getElementById","year","Date","getFullYear","textContent","handleSignIn","response","get","redirectTo","window","location","href","error","console"],"sources":["C:\\Users\\ZENK\\Desktop\\Vue_FrontendV2\\src\\components\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"banner\">\r\n    <div style=\"margin-top: 13em; padding-inline:40%; z-index:1\">\r\n      <div class=\"form-signin d-flex align-items-center justify-content-center w-100 h-100 position-absolute\">\r\n      <!-- Renderizar el formulario según el estado formType -->\r\n      <form v-if=\"formType === 'signin'\" @submit.prevent=\"handleSignIn\" class=\"signin\">\r\n\r\n        <img class=\"mb-4\" src=\"logofooter.png\" alt=\"\" width=\"150em\" height=\"130em\">\r\n        <h1 class=\"h3 mb-3 fw-normal\"></h1>\r\n\r\n        <div class=\"form-floating\">\r\n          <input type=\"email\" class=\"form-control\" id=\"floatingInput\" placeholder=\"name@example.com\" v-model=\"email\">\r\n          <label for=\"floatingInput\">Email</label>\r\n        </div>\r\n        <div class=\"form-floating\">\r\n          <input type=\"password\" class=\"form-control\" id=\"floatingPassword\" placeholder=\"Password\" v-model=\"password\">\r\n          <label for=\"floatingPassword\">Contraseña</label>\r\n        </div>\r\n\r\n        <div class=\"form-check text-start my-3\">\r\n          <input class=\"form-check-input\" type=\"checkbox\" value=\"remember-me\" id=\"flexCheckDefault\">\r\n          <label class=\"form-check-label\" for=\"flexCheckDefault\">\r\n            Recordar\r\n          </label>\r\n        </div>\r\n        <button class=\"btn btn-primary w-100 py-2\" style=\"background: linear-gradient(\r\n    to right,\r\n    #df18df,\r\n    #4c9ae7\r\n  );\" type=\"submit\">Iniciar Sesión</button>\r\n        <p class=\"mt-4 mb-3 text-body-secondary\"> &copy; 2023-<span id=\"currentYear\"></span>, Aritz Robledo</p>\r\n      </form>\r\n      <form v-else-if=\"formType === 'signup'\" @submit.prevent=\"createAccount\" class=\"signup\">\r\n        <!-- Contenido del formulario de registro -->\r\n      </form>\r\n      <form v-else @submit.prevent=\"handleForgotPassword\" class=\"forgot\">\r\n        <div class=\"text-center text-light mb-5 \">\r\n      <img class=\"mb-4\" src=\"logofooter.png\" alt=\"\" width=\"150em\" height=\"130em\" />\r\n      <h1>\r\n        <b>Kide</b>Tza\r\n      </h1>\r\n    </div>\r\n    <input\r\n      class=\"form-control mb-4\"\r\n      type=\"email\"\r\n      placeholder=\"Emaila\"\r\n      v-model=\"email\"\r\n    />\r\n    <button\r\n      type=\"button\"\r\n      class=\"btn btn-primary w-100 py-2 send\"\r\n      @click=\"handleForgotPassword\"\r\n    >\r\n      Berreskuratu\r\n    </button>\r\n    <button\r\n      type=\"button\"\r\n      class=\"SignIn w-100 py-2\"\r\n      @click=\"setFormType('signin')\"\r\n    >\r\n      Sartu\r\n    </button>\r\n    <p class=\"mt-5 mb-3 text-light\">\r\n      &copy;<b>Kide</b>Tza {{ currentYear }}\r\n    </p>\r\n      </form>\r\n    </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport axios from 'axios';\r\nimport Global from '../Global'\r\n\r\nexport default {\r\n  name: \"LoginView\",\r\n  setup() {\r\n    const url = ref(Global);\r\n    const data = ref(null);\r\n    const currentYear = ref(null);\r\n\r\n    onMounted(() => {\r\n      // Obtenemos el elemento del span que mostrará el año actual\r\n      currentYear.value = document.getElementById(\"currentYear\");\r\n      // Obtenemos el año actual\r\n      const year = new Date().getFullYear();\r\n      // Actualizamos el contenido del span con el año actual\r\n      currentYear.value.textContent = year;\r\n    });\r\n\r\n    const handleSignIn = async () => {\r\n      try {\r\n        const response = await axios.get(url.value + \"auth/login\");\r\n        data.value = response.data.data;\r\n        const redirectTo = response.data.redirectTo;\r\n        if (redirectTo) {\r\n          window.location.href = redirectTo; // Redirige a la página de autenticación de Twitch\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error al iniciar sesión:\", error);\r\n      }\r\n    };\r\n\r\n    return {\r\n      handleSignIn,\r\n      currentYear\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.banner {\r\n  position: relative;\r\n  background-size: cover;\r\n  padding: 10em;\r\n  height: 60vh;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n</style>"],"mappings":"AAyEA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,WAAU;AAE7B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,GAAE,GAAIN,GAAG,CAACG,MAAM,CAA